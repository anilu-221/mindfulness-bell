{
	"name": "Mindfulness Bell",
	"description": "May this bell bring you back to the present moment.",
	"version": "0.1.0",
	"manifest_version": 3,
	"icons": {
		"16": "src/img/icons/mb-icon-16.png",
		"48": "src/img/icons/mb-icon-48.png",
		"128": "src/img/icons/mb-icon-128.png"
	},    
	"background": {
		"service_worker": "background.js"
	},
	"permissions": [
		"alarms",
		"storage"
	],
	"action": {
		"default_popup": "popup.html",
		"detault_title": "Mindfulness Bell"
	}
}
